@model Tuple<CampusSecurity.Models.RankingsModel, CampusSecurity.Models.Discipline>


    <table class="w3-table w3-striped w3-border">
        <tr>
            <th>University name</th>
            <th>Branch</th>
            <th>City</th>
            <th>State</th>
            <th>Incidents</th>
        </tr>
       @foreach (var item in Model.Item1.answer)
       {
           <tr>
               <td>
                   @item.Uni_name
               </td>
               <td>
                   @item.Branch_name
               </td>
               <td>
                   @item.City_name
               </td>
               <td>
                   @item.State_name
               </td>
               <td>
                   @item.Incidents
               </td>
           </tr>
        
       }
   </table>
 



